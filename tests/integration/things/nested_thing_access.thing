thing InnerContainer
    has number val1
    has number val2

    setup
        Output.write("created InnerContainer")

thing Container
    has InnerContainer inner

    setup
        self.inner = create InnerContainer()
        self.inner.val1 = 1
        self.inner.val2 = 2

    does print
        Output.write(self.inner.val1)
        Output.write(self.inner.val2)

    does set with number a, number b
        self.inner.val1 = a
        self.inner.val2 = b


thing Program
    setup
        Container c = create Container()

        c.print()

        c.set(10, 20)
        c.print()

        c.inner.val1 = 100
        c.inner.val2 = 200
        c.print()

        Output.write(c.inner.val1)
        Output.write(c.inner.val2)