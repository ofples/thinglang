/*
{
    "expected_output": ["Created InnerContainer", "1", "2", "10", "20", "100", "200", "100", "200", "Printing InnerContainer", "100", "15", "Created InnerContainer", "Printing InnerContainer", "999"]
}
*/

thing InnerContainer
    has number val1
    has number val2
    has InnerContainer nested

    setup
        Output.write("Created InnerContainer")
        self.val1 = 999

    does print
        Output.write("Printing InnerContainer")
        Output.write(self.val1)

    does get returns number
        return 15

    does make_inner
        self.nested = create InnerContainer()

thing Container
    has InnerContainer inner

    setup
        self.inner = create InnerContainer()
        self.inner.val1 = 1
        self.inner.val2 = 2

    does print
        Output.write(self.inner.val1)
        Output.write(self.inner.val2)

    does set with number a, number b
        self.inner.val1 = a
        self.inner.val2 = b


thing Program
    setup
        Container c = create Container()

        c.print()

        c.set(10, 20)
        c.print()

        c.inner.val1 = 100
        c.inner.val2 = 200
        c.print()

        Output.write(c.inner.val1)
        Output.write(c.inner.val2)

        c.inner.print()

        Output.write(c.inner.get())

        c.inner.make_inner()
        c.inner.nested.print()