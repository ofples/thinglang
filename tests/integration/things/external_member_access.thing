/*
{
    "expected_output": ["A container", "Still a container", "42", "43"]
}
*/

thing Container
    has text name
    has number value

    setup
        self.name = "A container"
        self.value = 42


thing Program
    setup
        Container c = create Container()

        Output.write(c.name)
        c.name = "Still a container"
        Output.write(c.name)

        Output.write(c.value)
        c.value = 43
        Output.write(c.value)